<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="gmail"
		class="com.learnfromexamples.springcore.session2.DI.episode2.ConstructorInjection.Gmail" />
	<bean id="watsapp"
		class="com.learnfromexamples.springcore.session2.DI.episode2.ConstructorInjection.WatsApp" />

	 <bean id="galley"
		class="com.learnfromexamples.springcore.session2.DI.episode2.ConstructorInjection.GalleryApp">
		<constructor-arg>
			<ref bean="gmail" />
		</constructor-arg>
	</bean>
</beans> 
