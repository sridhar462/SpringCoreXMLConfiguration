<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="employee"
		class="com.learnfromexamples.springcore.session2.DI.episode2.ConstructorInjection.Employee">
		<constructor-arg>
			<value>Sridharan</value>
		</constructor-arg>
		
		
		<constructor-arg>
			<value>chennai</value>
		</constructor-arg>
	
		<constructor-arg>
			<value>28</value>
		</constructor-arg>
	</bean>
</beans> 
